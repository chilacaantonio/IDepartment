<div class="col-md-6 col-md-offset-3">
<div class="row">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h2>Registrate</h2>
    </div>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.label "Nombre" %><br />
    <%= f.text_field :name, autofocus: true , required: true%>
  </div>
  <div class="field">
    <%= f.label "Apellido paterno" %><br />
    <%= f.text_field :paterno, autofocus: true , required: true%>
  </div>
  <div class="field">
    <%= f.label "Apellido materno" %><br />
    <%= f.text_field :materno, autofocus: true , required: true%>
  </div>

  <div class="field">
    <%= f.label "Fecha de nacimiento" %><br />
    <%= f.date_select :edad,:start_year=>1996,:end_year=>2000%>
  </div>

  <div class="field">
    <%= f.label "Escuela" %><br />
    <%= f.text_field :escuela, autofocus: true , required: true%>
  </div>


  <div class="field">
    <%= f.label "Correo Electronico" %><br />
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label "ContraseÃ±a" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="field">
    <%= f.label "Confirmar contraseÃ±a" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off" %>
  </div>
<br>
  <div class="actions">
    <%= f.submit "Registrar", class: "btn btn-default" %>
  </div>
<% end %>



</div>
</div>
</div>
