<% @imagen.each do |a| %>
  <% @datos.each do |dat| %>
    <% if @imagen[@imagen.index(a)] == dat.cual %>
      <div class="panel panel-default">
          <div class="panel-body">
            <div class="row">
              <center><%=image_tag(@imagen[@imagen.index(a).to_i]+".png", alt: "Example Image", class: "img-responsive")%><br>
                <h2><%= @titulo[@imagen.index(a).to_i] %></h2><br>
                <h1><%= @sub[@imagen.index(a).to_i] %></h1><br>
                <strong>Miembros de la mesa:</strong><%= @quien[@imagen.index(a).to_i] %><br>
                <strong>Nivel:</strong><%= @level[@imagen.index(a).to_i] %><br>
                <strong>Grado:</strong> <%= @grade[@imagen.index(a).to_i] %><br><br>
                <% if @imagen[@imagen.index(a).to_i] == "fao" %>
                <strong><u>DELEGATE: If you’re on 2nd grade, you will be able to participate in pairs in High School Committees that allow pairs, you’ll find the High School Committees you can participate in on the App!</u></strong>
                <% end %>
                <br><br></center>
              </div></div></div><br><br>
    <% end %>
  <% end %>
<% end %>
